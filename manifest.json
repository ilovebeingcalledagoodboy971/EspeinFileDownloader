{
  "manifest_version": 2,
  "name": "EpsteinFilesDownloader",
  "version": "1.0",
  "permissions": [
    "downloads",
    "activeTab",
    "<all_urls>"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ]
}
